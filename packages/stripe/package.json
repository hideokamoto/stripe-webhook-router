{
  "name": "@tayori/stripe",
  "version": "0.1.0",
  "description": "Stripe-specific type definitions and utilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "check-events": "tsx scripts/check-events.ts"
  },
  "dependencies": {
    "@tayori/core": "workspace:*"
  },
  "devDependencies": {
    "stripe": "^20.3.1",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "stripe": ">=17.0.0"
  },
  "keywords": [
    "stripe",
    "webhook",
    "router",
    "typescript"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/stripe-webhook-router"
  },
  "homepage": "https://github.com/hideokamoto/stripe-webhook-router#readme",
  "bugs": "https://github.com/hideokamoto/stripe-webhook-router/issues"
}
