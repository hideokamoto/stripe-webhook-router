{
  "name": "@and-subscribe/express",
  "version": "0.1.0",
  "description": "Express adapter for Stripe Webhook Router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@and-subscribe/core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "stripe": "^20.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "stripe": ">=14.0.0"
  },
  "keywords": ["stripe", "webhook", "router", "express", "typescript"],
  "license": "MIT"
}
