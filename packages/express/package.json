{
  "name": "@tayori/express",
  "version": "0.1.0",
  "description": "Express adapter for Tayori Webhook Router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tayori/core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "tsup": "^8.0.0",
    "vitest": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "keywords": [
    "webhook",
    "router",
    "express",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/stripe-webhook-router"
  },
  "homepage": "https://github.com/hideokamoto/stripe-webhook-router#readme",
  "bugs": "https://github.com/hideokamoto/stripe-webhook-router/issues"
}
