{
  "name": "@and-subscribe/lambda",
  "version": "0.1.0",
  "description": "AWS Lambda adapter for Stripe Webhook Router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@and-subscribe/core": "workspace:*"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "stripe": "^20.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "stripe": ">=14.0.0"
  },
  "keywords": ["stripe", "webhook", "router", "lambda", "aws", "typescript"],
  "license": "MIT"
}
