{
  "name": "@tayori/lambda",
  "version": "0.0.1",
  "description": "AWS Lambda adapter for Tayori Webhook Router",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tayori/core": "workspace:*"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "tsup": "^8.0.0",
    "vitest": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "webhook",
    "router",
    "lambda",
    "aws",
    "typescript"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/stripe-webhook-router"
  },
  "homepage": "https://github.com/hideokamoto/stripe-webhook-router#readme",
  "bugs": "https://github.com/hideokamoto/stripe-webhook-router/issues"
}
